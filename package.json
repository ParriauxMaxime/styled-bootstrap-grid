{
  "name": "styled-bootstrap-grid",
  "version": "2.0.1",
  "description": "bootstrap grid system using styled components",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "build": "npm run clean && npm run build:types && npm run build:js",
    "clean": "rm -rf dist/",
    "build:types": "tsc --emitDeclarationOnly",
    "build:js": "babel src --out-dir dist --extensions \".ts,.tsx\"",
    "prepublishOnly": "npm run build",
    "test": "npm run build && jest",
    "format": "prettier --write 'src/**/*.{ts,tsx,js,jsx,json}'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dragma/styled-bootstrap-grid.git"
  },
  "keywords": [
    "styled-components",
    "bootstrap",
    "react-bootstrap",
    "layout",
    "grid",
    "grid system",
    "styled grid",
    "styled layout",
    "styled bootstrap grid"
  ],
  "author": "Florent BÃ©jina",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dragma/styled-bootstrap-grid/issues"
  },
  "homepage": "https://github.com/dragma/styled-bootstrap-grid#readme",
  "peerDependencies": {
    "react": "^0.14.0 || ^16.0.0-0",
    "styled-components": ">=4"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.5",
    "@babel/core": "^7.1.6",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.1.0",
    "@types/jest": "^24.0.0",
    "@types/react": "^16.7.11",
    "@types/react-dom": "^16.0.11",
    "@types/styled-components": "^4.1.2",
    "babel-jest": "^24.1.0",
    "chai": "^4.2.0",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-14": "^1.3.0",
    "enzyme-adapter-react-16": "^1.9.1",
    "eslint": "^5.9.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-jest-enzyme": "^7.0.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.11.1",
    "jest": "^24.1.0",
    "jsdom": "^13.2.0",
    "prettier": "^1.15.3",
    "react-addons-test-utils": "^15.6.2",
    "react-test-renderer": "^16.8.1",
    "ts-jest": "^23.10.5",
    "typescript": "^3.2.1"
  }
}
